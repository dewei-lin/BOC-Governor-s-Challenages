lambda = bv_lambda(mode = 0.2, sd = 0.4, min = 0.0001, max = 5),
alpha = bv_alpha(mode = 2), var = 1e07)
soc <- bv_soc(mode = 1, sd = 1, min = 1e-04, max = 50)
sur <- bv_sur(mode = 1, sd = 1, min = 1e-04, max = 50)
priors <- bv_priors(hyper = "auto", mn = mn, soc = soc, sur = sur)
mh <- bv_metropolis(scale_hess = c(0.05, 0.0001, 0.0001),
adjust_acc = TRUE, acc_lower = 0.25, acc_upper = 0.45)
bvar <- bvar(dat, lags = 4, n_draw = 15000, n_burn = 5000, n_thin = 15,
priors = priors, mh = mh, by_fcast =12,
verbose = TRUE)
bvar <- bvar(dat, lags = 3, n_draw = 15000, n_burn = 5000, n_thin = 15,
priors = priors, mh = mh, by_fcast =12,
verbose = TRUE)
bvar <- bvar(dat, lags = 2, n_draw = 15000, n_burn = 5000, n_thin = 15,
priors = priors, mh = mh, by_fcast =12,
verbose = TRUE)
bvar <- bvar(dat, lags = 1, n_draw = 15000, n_burn = 5000, n_thin = 15,
priors = priors, mh = mh, by_fcast =12,
verbose = TRUE)
predict(bvar)<-predict(bvar,horizon=12)
predict(bvar)$quants
